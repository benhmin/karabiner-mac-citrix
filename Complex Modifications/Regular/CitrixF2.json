{
    "description": "Maps the F2 to Command-F2 for use in Citrix",
    "manipulators": [
        {
            "conditions": [
                {
                    "bundle_identifiers": [
                        "com.citrix.XenAppViewer",
                        "com.citrix.receiver.icaviewer.mac"
                    ],
                    "type": "frontmost_application_unless"
                }
            ],
            "from": { "key_code": "f2" },
            "to": [{ "key_code": "display_brightness_increment" }],
            "type": "basic"
        }
    ]
}
